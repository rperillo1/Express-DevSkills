<%- include('../partials/header') %>


<body>
    <h3><%= skill.skill %></h3>
    <h3>Proficient: <%= skill.done ? 'Yes' : 'No' %></h3>
    <form action="/skills/<%= skill.id %>?_method=PUT" method="POST">
        <input type="text" name='skill' value='<%= skill.skill %>'>
        <button type='submit'>Submit</button>
    </form>
    <br>
    <a href="/skills">Back to Skills</a>
</body>


<%- include('../partials/footer') %>