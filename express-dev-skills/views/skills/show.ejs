<%- include('../partials/header') %>


<body>
    <h3>Skill # <%= skillNum %>: <%= skill.skill %></h3>
    <h3>Proficient: <%= skill.done ? 'Yes' : 'No' %></h3>
    <form action="/skills/<%= skill.id %>?_method=DELETE"
        class="delete-form" method="POST">
        <button type="submit">Delete Skill</button>
    </form>
    <a href="/skills">Back to Skills</a>
</body>


<%- include('../partials/footer') %>